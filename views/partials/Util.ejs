<%
    let dateToText = function (date) {
          let day = date.getDate().toString();
          let month = date.getMonth() + 1 + "";
          let year = date.getFullYear();
          if (day.length < 2){
            day = 0  + "" + day;
          }
          if (month.length < 2){
            month = 0 + "" + month;
          }
          return day + "/" + month + '/' + year;
        };
%>



